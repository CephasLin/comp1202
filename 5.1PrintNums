using System;
/*
 * Write a program to request an integer n from 1 to 9
 * and print a line of output consisting of ascending digits from 1 to n. 
 * For example, if n = 5, print the line
    12345

*/

namespace PrintNums
{
    class MainClass
    {
        public static void Main(string[] args)
        {

            int n,i;
            Console.WriteLine("Enter an integer:");
            n = Convert.ToInt32(Console.ReadLine());
            for(i=1; i<=n; i++)
            {
                Console.Write("{0}", i);
            }
            /*
