                3 x 1 = 3
                3 x 2 = 6
                3 x 3 = 9
                3 x 4 =12
                â€¦
                3 x 12 =36

            */

            int m;
            Console.WriteLine("Enter an integer:");
1            m = Convert.ToInt32(Console.ReadLine());
            for(i=1; i <=12; i++)
            {
                Console.WriteLine("{0} x {1} = {2}", m, i, m * i);
            }

        }
    }
}
